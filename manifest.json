    "action": {
        "default_icon": {
            "16": "/logo/logo-16.png",
            "32": "/logo/logo-32.png",
            "48": "/logo/logo-48.png",
            "128": "/logo/logo-128.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
